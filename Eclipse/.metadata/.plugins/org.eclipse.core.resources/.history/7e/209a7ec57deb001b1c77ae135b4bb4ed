#include "Mydrivers/timers.h"

void func(){
    PORTC^=0X01;
}

void main()
{

    DDRC=0X01;
    Timer_0_2_ConfigType cfg={F_CPU_1024,255,CTC,NORMAL_OCn_DISCONNECTED};
    Timer_2_init(&cfg);
    Timer_2_EnableCompareInterrupt();
    Timer_2_setCompareCallBack(func);  /*Set compare mode isr callback*/
    SREG=(1<<7);
    while(1);


}


